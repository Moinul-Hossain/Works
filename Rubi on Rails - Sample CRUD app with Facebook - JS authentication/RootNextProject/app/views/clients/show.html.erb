<%= field_set_tag 'Client Information' do %>
<p>
  <strong>Name:</strong>
  <%= @client.name %>
</p>
 
<p>
  <strong>Contact Name:</strong>
  <%= @client.contact_name %>
</p>

<p>
  <strong>Contact Phone:</strong>
  <%= @client.contact_phone %>
</p>
 
<p>
  <strong>Contact Email:</strong>
  <%= @client.contact_email %>
</p>

<p>
  <strong>Facebook Email:</strong>
  <%= @client.facebook_email %>
</p>
 
<p>
  <strong>Facebook Account:</strong>
  <%= @client.facebook_account %>
</p>
<%= link_to 'Back to the List', clients_path %>
|
<%= link_to 'Edit Client', edit_client_path(@client) %>
<% end %>